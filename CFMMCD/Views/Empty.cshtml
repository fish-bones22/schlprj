@using CFMMCD.Models.ViewModel
@{
    ViewBag.Title = "Report Generation";
}
<div class="center">
    <h2>@ViewBag.Title</h2>
</div>
<div class="container">
    @if (TempData["ErrorMessage"] != null)
    {
        <div class="alert alert-danger alert-dismissable fade in">@TempData["ErrorMessage"]<button class="close" data-dismiss="alert" aria-label="close">&times;</button></div>
    }
    @if (TempData["SuccessMessage"] != null)
    {
        <div class="alert alert-success alert-dismissable fade in">@TempData["SuccessMessage"]<button class="close" data-dismiss="alert" aria-label="close">&times;</button></div>
    }
</div>
<div class="container-fluid">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="row">
            <div class="col-lg-3">
                <div class="col-lg-8  col-lg-offset-4">
                    @Html.DropDownListFor(m => Model.storenumber, new SelectList(
                            new List<Object>{
                                new { value = "0" , text = "Store Number"  }
                            },
                            "value",
                            "text",
                            "0"), new { @class = "form-control input-sm", @Value = Model.storenumber })
                </div>
                <div class="col-lg-8 col-lg-offset-4">
                    <label class="control-label"> @Html.RadioButtonFor(m => m.LOS, "List of Stores")</label>
                </div>
            </div>
            <div class="col-lg-5">
                @Html.TextBoxFor(m => m.storename, new { @class = "form-control input-sm" })
                <div class="col-lg-3">
                    <label class="control-label">@Html.RadioButtonFor(m => m.LOS, "Items:")</label>
                </div>
                <div class="col-lg-3">
                    <label class="checkbox-inline control-label">@Html.CheckBoxFor(m => m.menuitem)Menu Item</label>
                </div>
                <div class="col-lg-3">
                    <label class="checkbox-inline control-label">@Html.CheckBoxFor(m => m.recipe)Recipe</label>
                </div>
                <div class="col-lg-3">
                    <label class="checkbox-inline control-label">@Html.CheckBoxFor(m => m.rawitem)Raw Items</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="col-lg-5">
                    <label class="control-label input-sm">Date From</label>
                </div>
                <div class="col-lg-7">
                    @Html.TextBoxFor(m => m.datefrom, new { @class = "form-control input-sm" })
                </div><br /><br />
                <div class="col-lg-5">
                    <label class="control-label input-sm">Date To</label>
                </div>
                <div class="col-lg-7">
                    @Html.TextBoxFor(m => m.dateto, new { @class = "form-control input-sm" })
                </div>
            </div>
        </div>
    }
</div>
<hr />
