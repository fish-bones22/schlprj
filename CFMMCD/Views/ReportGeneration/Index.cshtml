@model CFMMCD.Models.ViewModel.ReportGenerationViewModel
@using CFMMCD.Models.ViewModel
@{
    ViewBag.Title = "Report Generation";
}
<div class="center">
    <h2>@ViewBag.Title</h2>
</div><br />
<div class="container">
    @if (TempData["ErrorMessage"] != null)
    {
        <div class="alert alert-danger alert-dismissable fade in">@TempData["ErrorMessage"]<button class="close" data-dismiss="alert" aria-label="close">&times;</button></div>
    }
    @if (TempData["SuccessMessage"] != null)
    {
        <div class="alert alert-success alert-dismissable fade in">@TempData["SuccessMessage"]<button class="close" data-dismiss="alert" aria-label="close">&times;</button></div>
    }
</div>
<div class="container-fluid">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="row">
            <div class="col-lg-3">
                <div class="col-lg-8  col-lg-offset-4">
                    @Html.DropDownListFor(m => m.DUMMY, new SelectList(
                            new List<Object>{
                                new { value = "0" , text = "Store Number"  }
                            }, "value", "text", "0"), new { @class = "form-control input-sm" })
                </div>
                <div class="col-lg-8 col-lg-offset-4">
                    <label class="form-label2"> @Html.RadioButtonFor(m => m.DUMMY, "List of Stores") List of Stores</label>
                </div>
            </div>
            <div class="col-lg-5">
                @Html.TextBoxFor(m => m.DUMMY, new { @class = "form-control input-sm" })
                <div class="col-lg-3">
                    <label class="form-label2">@Html.RadioButtonFor(m => m.DUMMY, "Items:") Items:</label>
                </div>
                <div class="col-lg-3">
                    <label class="checkbox-inline form-label2">@Html.CheckBoxFor(m => m.DUMMYb)Menu Item</label>
                </div>
                <div class="col-lg-3">
                    <label class="checkbox-inline form-label2">@Html.CheckBoxFor(m => m.DUMMYb)Recipe</label>
                </div>
                <div class="col-lg-3">
                    <label class="checkbox-inline form-label2">@Html.CheckBoxFor(m => m.DUMMYb)Raw Items</label>
                </div>
            </div>
            <div class="col-lg-3">
                <div class="col-lg-5">
                    <label class="form-label2 input-sm">Date From</label>
                </div>
                <div class="col-lg-7">
                    @Html.TextBoxFor(m => m.DUMMY, new { @class = "form-control input-sm" })
                </div><br /><br />
                <div class="col-lg-5">
                    <label class="form-label2 input-sm">Date To</label>
                </div>
                <div class="col-lg-7">
                    @Html.TextBoxFor(m => m.DUMMY, new { @class = "form-control input-sm" })
                </div>
            </div>
        </div>
    }
</div>
<hr />
