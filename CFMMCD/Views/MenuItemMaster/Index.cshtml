                                </label>
                                <label class="form-label">
                                    @Html.CheckBoxFor(m => Model.SelectExcept)
                                    Select except
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <table class="table table-responsive">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th>Eat in</th>
                                    <th>Take out</th>
                                    <th>Non-product</th>
                                    <th>Others</th>
                                </tr>
                            </thead>
